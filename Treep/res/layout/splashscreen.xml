<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="@drawable/splashscreenbg" >
    
    
    <ImageView
        android:id="@+id/splashscreenlogo"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="25dp"
        android:src="@drawable/logosplash" />
    
    <ImageView 
		android:id="@+id/pintreepin" 
		android:layout_height="200dp" 
		android:src="@drawable/pintreepin"
		android:layout_marginLeft="-10dp"
        android:layout_toRightOf="@id/splashscreenlogo"
		android:layout_width="wrap_content"
		android:layout_centerVertical="true" />

    

</RelativeLayout>
